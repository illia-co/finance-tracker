{
  "name": "finance-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:connect": "docker exec -it finance-tracker-postgres-1 psql -U postgres -d finance_tracker",
    "db:backup": "docker exec finance-tracker-postgres-1 pg_dump -U postgres finance_tracker > backup.sql",
    "db:restore": "docker exec -i finance-tracker-postgres-1 psql -U postgres -d finance_tracker < backup.sql",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "portfolio:clear": "tsx scripts/clear-portfolio.ts",
    "portfolio:demo": "tsx scripts/demo-data.ts",
    "portfolio:reset": "tsx scripts/clear-portfolio.ts && tsx scripts/demo-data.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "next": "14.0.4",
    "postcss": "^8",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2"
  }
}
